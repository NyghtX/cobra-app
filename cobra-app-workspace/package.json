{
  "name": "cobra-app-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project dev-env",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "compodoc": "npx compodoc -p projects/cobra-app/tsconfig.doc.json -c projects/cobra-app/.compodocrc.json",
    "build-package": "ng-packagr -p projects/cobra-app/ng-package.json"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/projects/cobra-app/tsconfig.spec.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "projects/cobra-app/src/lib/**/*.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.3",
    "@angular/cdk": "^13.0.3",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "~13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.0.3",
    "@angular/material": "^13.0.3",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "~13.0.3",
    "@angular/router": "~13.0.3",
    "@types/jest": "^27.0.2",
    "rxjs": "~6.6.0",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.3",
    "@compodoc/compodoc": "^1.1.15",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "jest": "^27.3.1",
    "jest-preset-angular": "^11.0.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.8",
    "typescript": "~4.4.4"
  }
}
